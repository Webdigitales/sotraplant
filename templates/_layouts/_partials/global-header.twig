{# Admin bar plugin #}
{{ adminbar() }}

{# Site header #}
{% set companyCTA = craft.app.globals.getSetByHandle('siteCTA') %}
{% if companyCTA|length %}
	{% if companyCTA.cta.heading|length %}
		<div class="site-flashnews flashnews">
			{% if companyCTA.cta.button.url|length %}
			<a href="{{ companyCTA.cta.button.url }}" class="flashnews__link link--wrapper">
				{% endif %}
				<div class="flashnews__content">
					{% include '_components/tag.twig' with {'entity': companyCTA.cta} %}
					<p class="flashnews__message">{{ companyCTA.cta.heading }}</p>
				</div>
				{% if companyCTA.cta.button.url|length %}
			</a>
			{% endif %}
		</div>
	{% endif %}
{% endif %}
{% if companyInfo.companyGlobalPhone|length %}
<div class="header__phone lg:invisible">
	<a href="tel:{{ companyInfo.companyGlobalPhone.international }}"
		 class="link link-phone">
